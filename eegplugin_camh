function eegplugin_camh( fig, try_strings, catch_strings )

toolsmenu = findobj(fig, 'tag', 'tools');
submenu = uimenu( toolsmenu, 'label', 'OPEN FILE');

if nargin < 3
    error('eegplugin_camh requires 3 arguments');
end

uimenu( submenu, 'label', 'OPEN FILE', ...
    'callback', [try_strings.no_check 'LASTCOM = pop_camh(EEG);' catch_strings.add_to_hist]);

end
